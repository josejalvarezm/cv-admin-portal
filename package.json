{
  "name": "cv-admin-portal",
  "version": "0.1.0",
  "private": true,
  "description": "Admin portal for managing CV data across D1CV and cv-ai-agent databases",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test --ignore-snapshots",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:prod": "cross-env TEST_ENV=production playwright test",
    "test:e2e:codegen": "playwright codegen http://localhost:5173",
    "test:manual": "playwright test tests/manual-helpers.spec.ts --headed --project=chromium",
    "test:manual:add": "playwright test tests/manual-helpers.spec.ts -g \"Add NEW Technology\" --headed --project=chromium",
    "test:manual:edit": "playwright test tests/manual-helpers.spec.ts -g \"Edit FIRST\" --headed --project=chromium",
    "test:manual:workflow": "playwright test tests/manual-helpers.spec.ts -g \"Complete Workflow\" --headed --project=chromium"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@tanstack/react-query": "^5.59.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-router-dom": "^6.27.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "keywords": [
    "cv",
    "admin",
    "cloudflare",
    "d1",
    "react",
    "mui"
  ],
  "author": "Jose Alvarez",
  "license": "MIT"
}